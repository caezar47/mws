{% if view=="header" %}
	<header class="{{prefix}}header">
		<div class="{{prefix}}header-container container">
			<div class="{{prefix}}header-row row">
				<div class="{{prefix}}header-cols cols  is--hamburger">
					<button class="{{prefix}}header-hamburger" data-hamburger>
						<span class="{{prefix}}header-hamburger-inner">
							<span class="{{prefix}}header-hamburger-line  is--left"></span>
							<span class="{{prefix}}header-hamburger-line  is--center"></span>
							<span class="{{prefix}}header-hamburger-line  is--right"></span>
						</span>
					</button>
				</div>
				<div class="{{prefix}}header-cols cols  is--logotip">
					<a class="{{prefix}}header-logotip" href="{{page.index}}">MWS Agency</a>
				</div>
				<div class="{{prefix}}header-cols cols  is--right">
					<div class="{{prefix}}header-flex-row row">
						<div class="{{prefix}}header-flex-cols cols">
							<div class="{{prefix}}header-user  dropdown">
								<a class="{{prefix}}header-user-dropdown" data-toggle="dropdown" href="#">
									<span>Семён Вишневский</span>
									{% include "../svg-sprite/svg.twig" with {icon:"icon-user"} %}
								</a>
								<ul class="{{prefix}}header-user-menu  dropdown-menu">
									<li><a href="{{page.profile}}">Мой профиль</a></li>
									<li><a href="#" class="is--logout">Выйти</a></li>
								</ul>
							</div>
						</div>
						{% if basket %}
						<div class="{{prefix}}header-flex-cols cols">
							<a class="{{prefix}}header-basket" href="{{page.basket}}">
								<span class="{{prefix}}header-basket-name">{{basket}}</span>
								<span class="{{prefix}}header-basket-icon">
									{% include "../svg-sprite/svg.twig" with {icon:"icon-basket"} %}
								</span>
								{% if basket_qty %}
								<span class="{{prefix}}header-basket-qty">{{basket_qty}}</span>
								{% endif %}
								{% if basket_cost %}
								<span class="{{prefix}}header-basket-cost">{{basket_cost}} ₽</span>
								{% endif %}
							</a>
						</div>
						{% endif %}
						<div class="{{prefix}}header-flex-cols cols  is--resize">
							<button type="button" class="{{prefix}}header-resize" data-resize></button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>
{% endif %}
