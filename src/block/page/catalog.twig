{% if view == "page" %}
	<section class="{{content_prefix}}content">
		<div class="{{content_prefix}}container container">
			{% include "../common/page-header/elem.twig" with {
				prefix:"page-header__", 
				mod:heading_mod,
				attr:heading_attr, 
				level:heading_level, 
				heading:heading
			} %}
			<div class="{{content_prefix}}row  row  {{content_mod}}">
				<div class="cols  is--search">
					{% include "../common/form/elem.twig" with {
						view: "search", 
						prefix:'form__', 
						mod:'', 
						color:'',
						placeholder:"Поиск по каталогу",
					} %}
				</div>
				<div class="cols  hidden--md">
					{% include "../common/button/btn.twig" with {
						view:"icon",
						prefix:"btn__", 
						mod:"", 
						icon:'icon-upload', 
						link:"#", 
						name:"Скачать" 
					} %}
